<% include partials/header.ejs %>

<h1>Welcome to the posts page.</h1>

<% for(var i = 0; i < posts.length; i++) { %>
  <li><%= posts[i].title %> - <%= posts[i].author %></li>
<% } %> 

<br>

<% posts.forEach(function(post) { %>
  <li><%= post.title %> - <%= post.author %></li>
<% }); %>

